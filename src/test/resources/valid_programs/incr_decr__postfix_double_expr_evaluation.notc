// Expressions should evaluate to previous values

void main() {
    double d1, d2;

    d1 = 75.0;
    d2 = d1++;
    printDouble(d2);

    d1 = 0.0;
    d2 = d1--;
    printDouble(d2);

    d1 = 3.0;
    d2 = 7.0;
    printDouble(d1++ + d2++);
    printDouble(d1-- + d2--);
    printDouble(d1++ - d2--);
}
